{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Program",
            "args": [
                "--inspect=0.0.0.0:5858",
                "-r",
                "ts-node/register",
                "src/index.ts",
                "--in",
                "${workspaceFolder}/src/__tests__/test-data-array.json",
                "--out",
                "${workspaceFolder}/result.json",
                "--detection-keys",
                "_id",
                "--resolution-key",
                "entryDate"
            ],
            "request": "launch",
            "skipFiles": ["<node_internals>/**"],
            "type": "pwa-node"
        },

        {
            "type": "node",
            "name": "vscode-jest-tests",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/jest/bin/jest",
            "args": ["--runInBand"],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "disableOptimisticBPs": true
        }
    ]
}
